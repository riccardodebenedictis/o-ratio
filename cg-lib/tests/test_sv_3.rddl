class SV0 : StateVariable {

    SV1 sv1;

    SV0(SV1 sv1) : sv1(sv1) {
    }

    predicate P1() {
        duration == 5.0;
        goal p2 = new P2(start:end);
    }

    predicate P2() {
        duration == 8.0;
        goal p3 = new P3(start:end);
    }

    predicate P3() {
        duration == 8.0;
        goal p4 = new P4(start:end);
    }

    predicate P4() {
        duration == 10.0;
        {
            goal p1 = new P1(start:end);
        } or {
            goal p2 = new P2(start:end);
        }
        goal p6 = new sv1.P6();
        start >= p6.start;
        end <= p6.end;
    }
}

class SV1 : StateVariable {

    predicate P5() {
        duration >= 5.0;
        goal p6 = new P6(start:end);
    }

    predicate P6() {
        duration >= 8.0;
        {
            goal p7 = new P7(start:end);
        } or {
            goal p8 = new P8(start:end);
        }
    }

    predicate P7() {
        duration >= 8.0;
        goal p8 = new P8(start:end);
    }

    predicate P8() {
        duration >= 10.0;
        goal p5 = new P5(start:end);
    }
}

